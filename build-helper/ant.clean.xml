<project default="clean maven targets" basedir="../../zats">
	<property name="zats" location="${basedir}/zats/target" />
	<property name="common" location="${basedir}/zats-common/target" />
	<property name="example" location="${basedir}/zats-example/target" />
	<property name="mimic" location="${basedir}/zats-mimic/target" />
	<property name="mimic-ext6" location="${basedir}/zats-mimic-ext6/target" />

	<target name="clean maven targets">
		<delete dir="${zats}" />
		<delete dir="${common}" />
		<delete dir="${example}" />
		<delete dir="${mimic}" />
		<delete dir="${mimic-ext6}" />
	</target>
</project>